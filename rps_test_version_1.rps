stdlib = import "./rps_stdlib.rps";

wrapper = (func) -> {
    return ... -> {
        "before" -> $stdlib.writeLine;
        $parameters -> $func;
        "after" -> $stdlib.writeLine;
    };
};

a = {
    b = -110;
    c = (d, e) -> {
        ($b + $d * $e) -> $stdlib.writeLine;
    };
};

d = $a.c -> $wrapper;
(5, 2) -> $d;

test = (a, b, ...) -> {
    ("hello: %s%n", $parameters as list -> $stdlib.listToString) -> $stdlib.writeFormat;
};

(1, 2, 3, 4) -> $test;