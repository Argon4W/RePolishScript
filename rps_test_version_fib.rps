stdlib = import "./rps_stdlib.rps";

fibCache = [0, 1];

fib = (i) -> {
    for (index in 0 to ($i + 1)) {
        if ($index in $fibCache == null) {
            $index in $fibCache = ($index - 1) in $fibCache + ($index - 2) in $fibCache;
        };
    };
    return $i in $fibCache;
};

b = (10) -> $fib; //55
c = 8 in $fibCache; //21
d = 9 in $fibCache; //34

$b -> $stdlib.println;